{
  "compilerOptions": {
    "module": "ES2022",
    "moduleResolution": "nodenext",
    "target": "es2016",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    ]
}